<mvc:View
    controllerName="demo.controllers.TermsAndConds.Master"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:vm="sap.ui.comp.variants"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc">

    <f:DynamicPage class="sapUiContentPadding">

        <f:title>
            <f:DynamicPageTitle backgroundDesign="Transparent">
                <f:heading>
                    <vm:VariantManagement standardItemText="{appTxts>/tiles.titleTC}" />
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>

        <f:content>
            <html:iframe id="map_iframe" src="./assets/TermsAndConditions/termsandconditions.html" target="_top" width="100%" height="100%"/>
        </f:content>
    </f:DynamicPage>

</mvc:View>