<mvc:View displayBlock="true" 
        controllerName="demo.controllers.BoletinVta.DetailCargo" 
        height="100%"
		xmlns="sap.uxap"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:m="sap.m"
		xmlns:f="sap.f"
		xmlns:form="sap.ui.layout.form">

	<ObjectPageLayout id="ObjectPageLayout"
					showTitleInHeaderContent="true"
					alwaysShowContentHeader="false"
					preserveHeaderStateOnScroll="false"
					headerContentPinnable="true"
					isChildPage="true"
					upperCaseAnchorBar="false">

				<headerTitle>
					<ObjectPageDynamicHeaderTitle>
						<expandedHeading>
							<m:Title text="{appTxts>/polizas.Agreement} {debitDetModel>/Agreement}" wrapping="true" class="sapUiSmallMarginEnd"/>
						</expandedHeading>
						<snappedHeading>
							<m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
								<m:FlexBox wrap="NoWrap" fitContainer="true" alignItems="Center" class="sapUiTinyMarginEnd">
									<f:Avatar src="../../../../../../../{products>ProductPicUrl}" displaySize="S" displayShape="Square" class="sapUiTinyMarginEnd"/>
									<m:Title text="{promotionDetModel>/promotion}" wrapping="true" class="sapUiTinyMarginEnd"/>
								</m:FlexBox>
							</m:FlexBox>
						</snappedHeading>
                        <!-- <expandedContent>
                            <m:Text text="{promotionDetModel>/Description}"/>
                        </expandedContent>
                        <snappedContent>
                            <m:Text text="{promotionDetModel>/Description}"/>
                        </snappedContent> -->
						<navigationActions>
                            <!-- <m:OverflowToolbarButton type="Transparent" icon="sap-icon://receipt" press="handleReceipt"/> -->
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" id="enterFullScreenBtn" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"/>
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" id="exitFullScreenBtn" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"/>
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close column" visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }"/>
						</navigationActions>
					</ObjectPageDynamicHeaderTitle>
				</headerTitle>
		<sections>

			<ObjectPageSection >
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<m:Table sticky="HeaderToolbar,InfoToolbar,ColumnHeaders" 
                                    noDataText="{appTxts>/polizas.noData}" 
                                    id="debitDetTable"
                                    alternateRowColors="true" 
									items="{debitDet>/}">
                                     <m:headerToolbar>
                                        <m:OverflowToolbar>
                                            <m:Title text="{appTxts>/polizas.Position} ({debitDet>/length})" />
                                            <m:ToolbarSpacer/>
                                            <m:Button icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" 
                                                press=".buildExportTable"
                                                visible="{= ${promotionDet>/length} > 0}"
                                            />
                                        </m:OverflowToolbar>
                                    </m:headerToolbar>
								<m:columns>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Agreement}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Material}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Centro=Centro}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.DocMat}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Posicion}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Vendor}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.Costo}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                        <m:footer>
                                            <m:ObjectNumber id="OBNC"
                                                number="{
                                                    parts:[{path:'debTotDetModel>/TotCost'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}" />
                                        </m:footer>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.precio}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                        <m:footer>
                                            <m:ObjectNumber id="OBNP"
                                                number="{
                                                    parts:[{path:'debTotDetModel>/TotPrice'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}" />
                                        </m:footer>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.bonus}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                        <m:footer>
                                            <m:ObjectNumber id="OBNB"
                                                number="{
                                                    parts:[{path:'debTotDetModel>/TotBonus'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}" />
                                        </m:footer>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.IVA}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                        <m:footer>
                                            <m:ObjectNumber id="OBNT"
                                                number="{
                                                    parts:[{path:'debTotDetModel>/TotIVA'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}" />
                                        </m:footer>
                                    </m:Column>
                                    <m:Column width="auto" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                                        <m:header>
                                            <m:Text text="{appTxts>/PolizadetCargo.IEPS}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                                        </m:header>
                                        <m:footer>
                                            <m:ObjectNumber id="OBNE"
                                                number="{
                                                    parts:[{path:'debTotDetModel>/TotIEPS'},{path:'Currency'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}" />
                                        </m:footer>
                                    </m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem >
										<m:cells>
                                            <m:ObjectIdentifier title="{debitDet>Material}" 
                                                text="{debitDet>Agreement}" />
                                            <m:ObjectIdentifier title="{debitDet>Material}"
                                                text="{debitDet>MakDescription}" />
                                            <m:ObjectIdentifier title="{debitDet>Forum}"
                                                text="{debitDet>ForumDescription}" />
                                            <m:ObjectIdentifier
                                                text="{debitDet>MaterialDoc}" />
                                            <m:ObjectIdentifier
                                                text="{debitDet>MatdocPosition}" />
                                            <m:ObjectIdentifier title="{debitDet>Vendor}"
                                                text="{debitDet>VendorName}" />
                                            <m:ObjectNumber
                                                emphasized="true"
                                                number="{
                                                    parts:[{path:'debitDet>Cost'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}'"
                                                state="Success" />
                                            <m:ObjectNumber
                                                emphasized="true"
                                                number="{
                                                    parts:[{path:'debitDet>Price'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}'"
                                                state="Success" />
                                            <m:ObjectNumber
                                                emphasized="true"
                                                number="{
                                                    parts:[{path:'debitDet>Tax'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}'"
                                                state="Success" />
                                            <m:ObjectNumber
                                                emphasized="true"
                                                number="{
                                                    parts:[{path:'debitDet>Ieps'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="{debitDet>Currency}'"
                                                state="Success" />
                                            <!--
                                            <m:ObjectIdentifier
                                                text="{
                                                    path: 'AportacionesDet>Zieps',
                                                    type: 'sap.ui.model.type.Float'
                                                    }" />
                                            <m:ObjectNumber
                                                emphasized="true"
                                                number="{
                                                    parts:[{path:'AportacionesDet>Ztotal'},{path:'CurrencyCode'}],
                                                    type: 'sap.ui.model.type.Currency',
                                                    formatOptions: {showMeasure: false}
                                                        }"
                                                unit="MXN"
                                                state="Success" />
                                            <m:ObjectIdentifier
                                                text="{
                                                    path: 'AportacionesDet>Zaportacion',
                                                    type: 'sap.ui.model.type.Float'
                                                    }" />
                                            -->
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
    
</mvc:View>