<mvc:View displayBlock="true" 
    xmlns:f="sap.f" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar" 
    xmlns:vm="sap.ui.comp.variants" 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
	controllerName="demo.controllers.AcuerdosHNS.Master" height="100%">
	<f:DynamicPage  
		class="sapUiContentPadding">

        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <fb:FilterBar id="filterBar"  header="Header"
		                reset="onReset" search="searchData" clear="clearFilters" cancel="onCancel" 
		                filtersDialogClosed = "onFiltersDialogClosed"
		                showRestoreButton="false" showClearButton="false" useToolbar="false"> 
			
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="A" label="{appTxts>/acuerdosHNS.proveedor}" mandatory="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input id="supplierInput" placeholder="{appTxts>/acuerdosHNS.proveedor}" showSuggestion="true" valueHelpOnly="true" showValueHelp="true" valueHelpRequest=".onValueHelpRequest" value="{configSite>/supplierInput}" />
                                </fb:control>
                            </fb:FilterGroupItem>
                        
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="B" label="{appTxts>/acuerdosHNS.refer}" mandatory="true" partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input
                                        id="referenciaInput"
                                        placeholder="{appTxts>/acuerdosHNS.refer}"
                                        showSuggestion="true"
                                        maxLength="30"
                                        >
                                    </Input>
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>

	                </fb:FilterBar>
                </f:content>
            </f:DynamicPageHeader>
        </f:header>
		<f:content>
            <VBox>
			<Table 
                noDataText="{appTxts>/acuerdosHNS.noData}" 
                id="tableAcuerdosHNS"
                items="{AcuerdosHdrHNS>/}">
                <headerToolbar>
                    <OverflowToolbar>
                        <Title text="{appTxts>/acuerdosHNS.title} ({AcuerdosHdr>/length})" />
                        <ToolbarSpacer/>
                        <!--<OverflowToolbarButton type="Transparent" 
                            icon="sap-icon://pdf-attachment" 
                            press="onPressPDF" 
                            id="pdfCFDIBtn" 
                            tooltip="CFDI" 
                            visible="{= ${AcuerdosHdr>/UUID} === undefined || ${AcuerdosHdr>/UUID} === '' ? false : true }"/>-->
                        <Button icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" 
                            press=".buildExportTable"
                            visible="{= ${AcuerdosHdrHNS>/length} > 0}"
                        />
                        <!--<Select 
                            visible="{= ${AcuerdosHdr>/length} > 0 }"
                            change=".paginateValue(${$parameters>/selectedItem}, 'AcuerdosHdr', '/')">
                            <items>
                                <core:Item key="10" text="10"/>
                                <core:Item key="20" text="20"/>
                                <core:Item key="50" text="50"/>
                                <core:Item key="100" text="100"/>
                            </items>
                        </Select>-->
                    </OverflowToolbar>
                </headerToolbar>
				<columns>
                	<Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.centro}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <Text text="Total" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.canti}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotCanti'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="" />
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.cNorm}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotCnorm'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.cOfer}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotCofer'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.difer}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotDifer'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.bonif}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotBonif'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>
                    <Column width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.ieps}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotImpieps'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>
					<Column width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
						<header>
							<Text text="{appTxts>/acuerdosHNS.iva}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
						</header>
						<footer>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'acuTotHNSModel>/TotIva'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                        </footer>
					</Column>

				</columns>
				<items>
					<ColumnListItem type="Navigation" press="onListItemPress">                    
						<cells>
                            <ObjectIdentifier 
                                title="{AcuerdosHdrHNS>Werks}" 
                                titleActive="true"/>
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Canti'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Cnorm'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Cofer'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Difer'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Bonif'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Impieps'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
                            <ObjectNumber
                                number="{
                                    parts:[{path:'AcuerdosHdrHNS>Iva'},{path:'CurrencyCode'}],
                                    type: 'sap.ui.model.type.Currency',
                                    formatOptions: {showMeasure: false}
                                        }"
                                unit="MXN" />
						</cells>
					</ColumnListItem>
				</items>
			</Table>
            <!--<OverflowToolbar visible="{= ${AcuerdosHdr>/AcuerdosDet/Paginated/results/length} > 0 }">
                <Text text="{AcuerdosHdr>/AcuerdosDet/Paginated/initRow} - {AcuerdosHdr>/AcuerdosDet/Paginated/endRow} {appTxts>/pagination.of} {AcuerdosHdr>/AcuerdosDet/results/length}" />
                <ToolbarSpacer/>
                <Button type="Default" icon="sap-icon://slim-arrow-left" 
                        press=".prevPagination(${AcuerdosHdr>/AcuerdosDet/Paginated/iterator}, ${AcuerdosHdr>/AcuerdosDet/Paginated/initRow}, 'AcuerdosHdr', '/AcuerdosDet')" 
                        enabled="{= ${AcuerdosHdr>/AcuerdosDet/Paginated/iterator} !== 1}" />
                <Button type="Default" icon="sap-icon://slim-arrow-right" 
                        press=".nextPagination(${AcuerdosHdr>/AcuerdosDet/Paginated/iterator}, ${AcuerdosHdr>/AcuerdosDet/Paginated/endRow}, 'AcuerdosHdr', '/AcuerdosDet')" 
                        enabled="{= ${AcuerdosHdr>/AcuerdosDet/Paginated/endRow} !== ${AcuerdosHdr>/AcuerdosDet/results/length}}" />
            </OverflowToolbar>-->
            </VBox>
		</f:content>
	</f:DynamicPage>
</mvc:View>