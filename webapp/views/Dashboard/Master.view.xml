<mvc:View
    controllerName="demo.controllers.Dashboard.Master"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns:x="sap.uxap"
    xmlns:card="sap.f.cards"
    xmlns:l="sap.ui.layout"
    xmlns:mc="sap.suite.ui.microchart">

    <IconTabBar
        id="idIconTabBar"
        class="sapUiResponsiveContentPadding"
        expandable="false">
        <items>
            <IconTabFilter
                icon="sap-icon://manager-insight"
                text="{appTxts>/dashboard.tab.title.repGeneral}"
                key="repGeneral">
                <fb:FilterBar id="filterBar" header="Header" reset="onReset" search="searchData"
                    clear="clearFilters" cancel="onCancel"
                    filtersDialogClosed="onFiltersDialogClosed"
                    showRestoreButton="false" showClearButton="false" useToolbar="false"
                    filterBarExpanded="true">
                    <fb:filterGroupItems>
                        <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab"
                            label="{appTxts>/dashboard.repGeneral.filter.supplier}" mandatory="true"
                            partOfCurrentVariant="true" visibleInFilterBar="true">
                            <fb:control>
                                <Input
                                    id="supplierInput"
                                    placeholder="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                    showSuggestion="true" valueHelpOnly="true"
                                    showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
                                    value="{configSite>/supplierInput}">
                                </Input>
                            </fb:control>
                        </fb:FilterGroupItem>

                        <fb:FilterGroupItem groupName="__$INTERNAL$"
                            label="{appTxts>/dashboard.repGeneral.filter.date}"
                            groupTitle="FechaAlta" name="FechaAlta" partOfCurrentVariant="true"
                            visibleInFilterBar="true">
                            <fb:control>
                                <DateRangeSelection id="dateRange" change="onChangeDate" />
                            </fb:control>
                        </fb:FilterGroupItem>

                        <fb:FilterGroupItem groupName="__$INTERNAL$"
                            label="{appTxts>/dashboard.repGeneral.filter.tipo}"
                            groupTitle="Tipo" name="Tipo" partOfCurrentVariant="true"
                            visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox items="" showClearIcon="true">
                                    <core:Item key="" text="" />
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>

                        <fb:FilterGroupItem groupName="__$INTERNAL$"
                            label="{appTxts>/dashboard.repGeneral.filter.status}"
                            groupTitle="Estatus" name="Estatus" partOfCurrentVariant="true"
                            visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox selectedKey="T">
                                    <core:Item key="E" text="{appTxts>/dashboard.filter.estatusE}" />
                                    <core:Item key="H" text="{appTxts>/dashboard.filter.estatusH}" />
                                    <core:Item key="T" text="{appTxts>/dashboard.filter.estatusT}" />
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>

                        <fb:FilterGroupItem groupName="__$INTERNAL$"
                            label="{appTxts>/dashboard.repGeneral.filter.antiguedad}"
                            groupTitle="Antiguedad" name="Antiguedad" partOfCurrentVariant="true"
                            visibleInFilterBar="true">
                            <fb:control>
                                <ComboBox selectedKey="T">
                                    <core:Item key="1"
                                        text="{appTxts>/dashboard.filter.antiguedad1}" />
                                    <core:Item key="2"
                                        text="{appTxts>/dashboard.filter.antiguedad2}" />
                                    <core:Item key="3"
                                        text="{appTxts>/dashboard.filter.antiguedad3}" />
                                </ComboBox>
                            </fb:control>
                        </fb:FilterGroupItem>
                    </fb:filterGroupItems>
                </fb:FilterBar>

                <l:VerticalLayout>

                    <HeaderContainer scrollStep="300">
                        <TileContent footer="Cantidad recibidas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://receipt" />
                            </content>
                        </TileContent>
                        <TileContent footer="Importe recibidas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://lead" />
                            </content>
                        </TileContent>
                        <TileContent footer="Cantidad resueltas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://activity-2" />
                            </content>
                        </TileContent>
                        <TileContent footer="Importe resueltas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://payment-approval" />
                            </content>
                        </TileContent>
                        <TileContent footer="Cantidad pendientes">
                            <content>
                                <NumericContent value="35" icon="sap-icon://pending" />
                            </content>
                        </TileContent>
                        <TileContent footer="Importe pendientes">
                            <content>
                                <NumericContent value="35" icon="sap-icon://time-overtime" />
                            </content>
                        </TileContent>
                        <TileContent footer="Cantidad pagadas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://paid-leave" />
                            </content>
                        </TileContent>
                        <TileContent footer="Importe pagadas">
                            <content>
                                <NumericContent value="35" icon="sap-icon://expense-report" />
                            </content>
                        </TileContent>
                    </HeaderContainer>

