                    <Text text="Three segments" class="sapUiSmallMargin">
                        <layoutData>
                            <l:GridData span="XL12 L12 M12 S12" />
                        </layoutData>
                    </Text>
                    <FlexBox width="20rem" height="10rem" alignItems="Start"
                        justifyContent="SpaceBetween">
                        <items>
                            <mc:InteractiveDonutChart selectionChanged="onSelectionChanged">
                                <mc:segments>
                                    <mc:InteractiveDonutChartSegment label="Implementation Phase"
                                        value="40.0" displayedValue="40.0%" />
                                    <mc:InteractiveDonutChartSegment label="Design Phase"
                                        value="21.5"
                                        displayedValue="21.5%" />
                                    <mc:InteractiveDonutChartSegment label="Test Phase" value="38.5"
                                        displayedValue="38.5%" />
                                </mc:segments>
                            </mc:InteractiveDonutChart>
                        </items>
                    </FlexBox>
                </l:VerticalLayout>

            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://company-view"
                text="{appTxts>/dashboard.tab.title.repSolGroup}"
                key="repSolGroup">
                <Text text="Attachments go here ..." />
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://timesheet"
                text="{appTxts>/dashboard.tab.title.repAvgTime}"
                key="repAvgTime">
                <Text text="Notes go here ..." />
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://compare-2"
                text="{appTxts>/dashboard.tab.title.repCompExec}"
                key="repCompExec">
                <Text text="People content goes here ..." />
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://supplier"
                text="{appTxts>/dashboard.tab.title.repAclarProv}"
                key="repAclarProv">
                <Text text="People content goes here ..." />
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://idea-wall"
                text="{appTxts>/dashboard.tab.title.repAclarAnalyst}"
                key="repAclarAnalyst">
                <Text text="People content goes here ..." />
            </IconTabFilter>
        </items>
    </IconTabBar>

</mvc:View>