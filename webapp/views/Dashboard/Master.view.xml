<mvc:View
    controllerName="demo.controllers.Dashboard.Master"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns:x="sap.uxap"
    xmlns:card="sap.f.cards"
    xmlns:l="sap.ui.layout"
    xmlns:mc="sap.suite.ui.microchart"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:viz.data="sap.viz.ui5.data">
    <IconTabBar
        id="idIconTabBar"
        class="sapUiResponsiveContentPadding"
        expandable="false"
        stretchContentHeight="true">
        <items>
            <IconTabFilter
                icon="sap-icon://manager-insight"
                text="{appTxts>/dashboard.tab.title.repGeneral}"
                key="repGeneral">
                <ScrollContainer
                    height="100%"
                    width="100%"
                    horizontal="false"
                    vertical="true">

                    <fb:FilterBar header="Header" reset="onReset" search="searchData"
                        clear="clearFilters" cancel="onCancel"
                        filtersDialogClosed="onFiltersDialogClosed"
                        showRestoreButton="false" showClearButton="false" useToolbar="false"
                        filterBarExpanded="true">
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab"
                                label="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input
                                        placeholder="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                        showSuggestion="true" valueHelpOnly="true"
                                        showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
                                        value="{configSite>/supplierInput}" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.date}"
                                groupTitle="FechaAlta" name="FechaAlta" partOfCurrentVariant="true"
                                mandatory="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <DateRangeSelection id="dateAlta" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.tipo}"
                                groupTitle="Tipo" name="Tipo" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <ComboBox showSecondaryValues="true" id="cboxTipo"
                                        items="{TiposAclaracion>/Tipos}"
                                        showClearIcon="true">
                                        <core:ListItem key="{TiposAclaracion>TipAcla}"
                                            text="{TiposAclaracion>DesAcla}"
                                            additionalText="{TiposAclaracion>TipAcla}" />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.status}"
                                groupTitle="Estatus" name="Estatus" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="T" id="cboxEstatus">
                                        <core:Item key="E"
                                            text="{appTxts>/dashboard.filter.estatusE}" />
                                        <core:Item key="H"
                                            text="{appTxts>/dashboard.filter.estatusH}" />
                                        <core:Item key="T"
                                            text="{appTxts>/dashboard.filter.estatusT}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.antiguedad}"
                                groupTitle="Antiguedad" name="Antiguedad"
                                partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="1" id="cboxAntiguedad">
                                        <core:Item key="1"
                                            text="{appTxts>/dashboard.filter.antiguedad1}" />
                                        <core:Item key="2"
                                            text="{appTxts>/dashboard.filter.antiguedad2}" />
                                        <core:Item key="3"
                                            text="{appTxts>/dashboard.filter.antiguedad3}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>

                    <l:HorizontalLayout class="sapUiContentPadding">
                        <l:VerticalLayout class="sapUiLargeMarginEnd">
                            <Text text="{appTxts>/dashboard.repGeneral.graph.title.cant}"
                                class="sapUiSmallMargin">
                                <layoutData>
                                    <l:GridData span="XL12 L12 M12 S12" />
                                </layoutData>
                            </Text>
                            <FlexBox width="20rem" height="10rem" alignItems="Start"
                                justifyContent="SpaceBetween">
                                <items>
                                    <mc:InteractiveDonutChart id="donutCantidad"
                                        selectionChanged="onSelectionChanged">
                                        <mc:segments>
                                            <mc:InteractiveDonutChartSegment
                                                label="{appTxts>/dashboard.card.title.porcetajePend}"
                                                value="{CardsModel>/Porcentajependientes}"
                                                displayedValue="{CardsModel>/Porcentajependientes}%" />
                                            <mc:InteractiveDonutChartSegment
                                                label="{appTxts>/dashboard.card.title.porcetajeRes}"
                                                value="{CardsModel>/Porcentajeresueltas}"
                                                displayedValue="{CardsModel>/Porcentajeresueltas}%" />
                                        </mc:segments>
                                    </mc:InteractiveDonutChart>
                                </items>
                            </FlexBox>
                        </l:VerticalLayout>

                        <l:VerticalLayout class="sapUiLargeMarginBegin">
                            <Text text="{appTxts>/dashboard.repGeneral.graph.title.imp}"
                                class="sapUiSmallMargin">
                                <layoutData>
                                    <l:GridData span="XL12 L12 M12 S12" />
                                </layoutData>
                            </Text>
                            <FlexBox width="20rem" height="10rem" alignItems="Start"
                                justifyContent="SpaceBetween">
                                <items>
                                    <mc:InteractiveDonutChart id="donutImporte"
                                        selectionChanged="onSelectionChanged">
                                        <mc:segments>
                                            <mc:InteractiveDonutChartSegment
                                                label="{appTxts>/dashboard.card.title.impPend}"
                                                value="{CardsModel>/Porcentajependientes}"
                                                displayedValue="{CardsModel>/Importependientes}" />
                                            <mc:InteractiveDonutChartSegment
                                                label="{appTxts>/dashboard.card.title.impRes}"
                                                value="{CardsModel>/Porcentajeresueltas}"
                                                displayedValue="{CardsModel>/Importeresueltas}" />
                                        </mc:segments>
                                    </mc:InteractiveDonutChart>
                                </items>
                            </FlexBox>
                        </l:VerticalLayout>
                        <Button icon="sap-icon://excel-attachment"
                            text="{appTxts>/dashboard.download.excel}" press="downloadExcelGralRep" />
                    </l:HorizontalLayout>

                    <HeaderContainer id="container2" scrollStep="200">
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.recibidasCant}">
                            <content>
                                <NumericContent value="{CardsModel>/Cantrecibidas}"
                                    icon="sap-icon://receipt" />
                            </content>
                        </TileContent>
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.recibidasImp}">
                            <content>
                                <NumericContent value="{CardsModel>/Importerecibidas}"
                                    formatterValue="true"
                                    scale="M"
                                    icon="sap-icon://simple-payment" />
                            </content>
                        </TileContent>
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.resueltasCant}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Cantresueltas'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://activity-2" />
                            </content>
                        </TileContent>
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.resueltasImp}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Importeresueltas'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://payment-approval" />
                            </content>
                        </TileContent>
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.pendientesCant}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Cantpendientes'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://pending" />
                            </content>
                        </TileContent>
                        <TileContent
                            footer="{appTxts>/dashboard.repGeneral.card.title.pendientesImp}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Importependientes'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://time-overtime" />
                            </content>
                        </TileContent>
                        <TileContent footer="{appTxts>/dashboard.repGeneral.card.title.pagadasCant}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Cantpagadas'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://paid-leave" />
                            </content>
                        </TileContent>
                        <TileContent footer="{appTxts>/dashboard.repGeneral.card.title.pagadasImp}">
                            <content>
                                <NumericContent
                                    value="{
                                    parts: [{path: 'CardsModel>/Importepagadas'}], 
                                    type:'sap.ui.model.type.Currency', 
                                    formatOptions: {showMeasure:false}}"
                                    icon="sap-icon://loan" />
                            </content>
                        </TileContent>
                    </HeaderContainer>
                </ScrollContainer>
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://company-view"
                text="{appTxts>/dashboard.tab.title.repSolGroup}"
                key="repSolGroup">
                <ScrollContainer
                    height="100%"
                    width="100%"
                    horizontal="false"
                    vertical="true">

                    <fb:FilterBar header="Header" reset="onReset" search="searchData"
                        clear="clearFilters" cancel="onCancel"
                        filtersDialogClosed="onFiltersDialogClosed"
                        showRestoreButton="false" showClearButton="false" useToolbar="false"
                        filterBarExpanded="true">
                        <fb:filterGroupItems>
                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab"
                                label="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input
                                        placeholder="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                        showSuggestion="true" valueHelpOnly="true"
                                        showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
                                        value="{configSite>/supplierInput}" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.date}"
                                groupTitle="FechaAlta" name="FechaAlta" partOfCurrentVariant="true"
                                mandatory="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <DateRangeSelection id="dateAltaGR" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.grupoRes}"
                                groupTitle="Grupo" name="Grupo" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <ComboBox showSecondaryValues="true" id="cboxGrupoRes"
                                        items="{GruposModel>/Grupos}"
                                        showClearIcon="true">
                                        <core:ListItem key="{GruposModel>Idusua}"
                                            text="{GruposModel>Analista}"
                                            additionalText="{GruposModel>Idusua}" />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.tipo}"
                                groupTitle="Tipo" name="Tipo" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <ComboBox showSecondaryValues="true" id="cboxTipoGR"
                                        items="{TiposAclaracion>/Tipos}"
                                        showClearIcon="true">
                                        <core:ListItem key="{TiposAclaracion>TipAcla}"
                                            text="{TiposAclaracion>DesAcla}"
                                            additionalText="{TiposAclaracion>TipAcla}" />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.status}"
                                groupTitle="Estatus" name="Estatus" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="T" id="cboxEstatusGR">
                                        <core:Item key="E"
                                            text="{appTxts>/dashboard.filter.estatusE}" />
                                        <core:Item key="H"
                                            text="{appTxts>/dashboard.filter.estatusH}" />
                                        <core:Item key="T"
                                            text="{appTxts>/dashboard.filter.estatusT}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>
                        </fb:filterGroupItems>
                    </fb:FilterBar>

                    <l:HorizontalLayout class="sapUiContentPadding">
                        <l:Grid defaultSpan="XL12 L12 M12 S12">
                            <!-- Example with absolute and percentage values -->
                            <FlexBox width="40rem" height="10rem" alignItems="Center" class="sapUiSmallMargin">
                                <items>
                                    <mc:InteractiveBarChart id="barChar" labelWidth="30%"
                                        bars="{GpoResModel>/oData}">
                                        <mc:bars>
                                            <mc:InteractiveBarChartBar label="{GpoResModel>Descgrupo}: ${ path: 'GpoResModel>Importe', type: 'sap.ui.model.type.Float' }" 
                                                value="{GpoResModel>Cantidad}" 
                                                displayedValue="{GpoResModel>Cantidad}"/>
                                        </mc:bars>
                                    </mc:InteractiveBarChart>
                                </items>
                            </FlexBox>
                        </l:Grid>
                        <Button icon="sap-icon://excel-attachment"
                                text="{appTxts>/dashboard.download.excel}" press="onDownloadExcelGpoRes" />
                    </l:HorizontalLayout>

                </ScrollContainer>
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://fob-watch"
                text="{appTxts>/dashboard.tab.title.repAvgTime}"
                key="repAvgTime">
                <ScrollContainer
                    height="100%"
                    width="100%"
                    horizontal="false"
                    vertical="true">
                    <fb:FilterBar header="Header" reset="onReset" search="searchData"
                        clear="clearFilters" cancel="onCancel"
                        filtersDialogClosed="onFiltersDialogClosed"
                        showRestoreButton="false" showClearButton="false" useToolbar="false"
                        filterBarExpanded="true">
                        <fb:filterGroupItems>

                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab"
                                label="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input
                                        placeholder="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                        showSuggestion="true" valueHelpOnly="true"
                                        showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
                                        value="{configSite>/supplierInput}" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.date}"
                                groupTitle="FechaAlta" name="FechaAlta" partOfCurrentVariant="true"
                                mandatory="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <DateRangeSelection id="dateAltaAvgT" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.tipo}"
                                groupTitle="Tipo" name="Tipo" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <ComboBox showSecondaryValues="true" id="cboxTipoAvgT"
                                        items="{TiposAclaracion>/Tipos}"
                                        showClearIcon="true">
                                        <core:ListItem key="{TiposAclaracion>TipAcla}"
                                            text="{TiposAclaracion>DesAcla}"
                                            additionalText="{TiposAclaracion>TipAcla}" />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.status}"
                                groupTitle="Estatus" name="Estatus" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="T" id="cboxEstatusAvgT">
                                        <core:Item key="E"
                                            text="{appTxts>/dashboard.filter.estatusE}" />
                                        <core:Item key="H"
                                            text="{appTxts>/dashboard.filter.estatusH}" />
                                        <core:Item key="T"
                                            text="{appTxts>/dashboard.filter.estatusT}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </ScrollContainer>
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://compare-2"
                text="{appTxts>/dashboard.tab.title.repCompExec}"
                key="repCompExec">
                <ScrollContainer
                    height="100%"
                    width="100%"
                    horizontal="false"
                    vertical="true">
                    <fb:FilterBar header="Header" reset="onReset" search="searchData"
                        clear="clearFilters" cancel="onCancel"
                        filtersDialogClosed="onFiltersDialogClosed"
                        showRestoreButton="false" showClearButton="false" useToolbar="false"
                        filterBarExpanded="true">
                        <fb:filterGroupItems>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repEjecutivoC.filter.year}"
                                groupTitle="Year" name="Year" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="T" id="cboxEjercicioExecC">
                                        <core:Item key="1"
                                            text="{appTxts>/dashboard.repEjecutivoC.year}" />
                                        <core:Item key="2"
                                            text="{appTxts>/dashboard.repEjecutivoC.yearPre}" />
                                        <core:Item key="3"
                                            text="{appTxts>/dashboard.repEjecutivoC.both}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repEjecutivoC.filter.month}"
                                groupTitle="Date" name="Date" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <DateRangeSelection id="dpDateExecC" displayFormat="MMMM" valueFormat="MMMM"/>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab"
                                label="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                partOfCurrentVariant="true" visibleInFilterBar="true">
                                <fb:control>
                                    <Input
                                        placeholder="{appTxts>/dashboard.repGeneral.filter.supplier}"
                                        showSuggestion="true" valueHelpOnly="true"
                                        showValueHelp="true" valueHelpRequest=".onValueHelpRequest"
                                        value="{configSite>/supplierInput}" />
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.tipo}"
                                groupTitle="Tipo" name="Tipo" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <ComboBox showSecondaryValues="true" id="cboxTipoExecC"
                                        items="{TiposAclaracion>/Tipos}"
                                        showClearIcon="true">
                                        <core:ListItem key="{TiposAclaracion>TipAcla}"
                                            text="{TiposAclaracion>DesAcla}"
                                            additionalText="{TiposAclaracion>TipAcla}" />
                                    </ComboBox>
                                </fb:control>
                            </fb:FilterGroupItem>

                            <fb:FilterGroupItem groupName="__$INTERNAL$"
                                label="{appTxts>/dashboard.repGeneral.filter.status}"
                                groupTitle="Estatus" name="Estatus" partOfCurrentVariant="true"
                                visibleInFilterBar="true">
                                <fb:control>
                                    <Select selectedKey="T" id="cboxEstatusExecC">
                                        <core:Item key="E"
                                            text="{appTxts>/dashboard.filter.estatusE}" />
                                        <core:Item key="H"
                                            text="{appTxts>/dashboard.filter.estatusH}" />
                                        <core:Item key="T"
                                            text="{appTxts>/dashboard.filter.estatusT}" />
                                    </Select>
                                </fb:control>
                            </fb:FilterGroupItem>

                        </fb:filterGroupItems>
                    </fb:FilterBar>
                </ScrollContainer>
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://supplier"
                text="{appTxts>/dashboard.tab.title.repAclarProv}"
                key="repAclarProv">
                <Text text="People content goes here ..." />
            </IconTabFilter>
            <IconTabFilter
                icon="sap-icon://idea-wall"
                text="{appTxts>/dashboard.tab.title.repAclarAnalyst}"
                key="repAclarAnalyst">
                <Text text="People content goes here ..." />
            </IconTabFilter>
        </items>
    </IconTabBar>
</mvc:View>