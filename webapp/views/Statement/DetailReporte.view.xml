                            </t:template>
                        </t:Column>
                        <t:Column width="auto" hAlign="Left">

                            <Text text="Docto. Material" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>

                            <t:template>
                                <Text text="{MejorCondHdr>TeSalida/DocMaterial}" wrapping="false"/>

