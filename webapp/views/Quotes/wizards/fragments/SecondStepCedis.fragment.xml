<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
    <VBox>
    <Panel visible="{	path: 'TemporalModel>/generalData/cedisType',
                        formatter: '.formatter.isSeco'
                    }">
        <Table mode="MultiSelect" id="tableWizardOrder" 
        items="" selectionChange=".selectChange">
            <headerToolbar>
            <OverflowToolbar>
                <ToolbarSpacer/>
                <Input id="searchOrders" width="auto" textAlign="Initial" placeholder="{appTxts>/quotes.search}" 
                    type="Number" showValueHelp="true" valueHelpIconSrc="sap-icon://search"/>
                </OverflowToolbar>
            </headerToolbar>
            <columns>
                <Column>
                    <Text text="{appTxts>/quotes.order}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column>
                    <Text text="{appTxts>/quotes.startShipment}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column>
                    <Text text="{appTxts>/quotes.endShipment}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column>
                    <Text text="{appTxts>/quotes.prevQuotes}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column>
                    <Text text="{appTxts>/quotes.pendingPos}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column>
                    <Text text="{appTxts>/quotes.appointPos}" width="auto" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <ObjectIdentifier text=""/>
                        <ObjectIdentifier text=""/>
                        <ObjectIdentifier text=""/>                                                       
                        <ObjectIdentifier text=""/>
                        <ObjectIdentifier text=""/>
                        <Input enabled="false"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Panel>


    <Panel visible="{	path: 'TemporalModel>/generalData/cedisType',
                        formatter: '.formatter.isFresco'
                    }">
        <Table mode="MultiSelect" id="tableWizardOrderPosition" 
        items="{path : 'tableWizardPo_validas>/Oekponav/results'}"
        selectionChange=".selectChange" >
            <headerToolbar>
                <OverflowToolbar>
                    <Input id="searchOrder" textAlign="Initial" placeholder="{appTxts>/quotes.order}" type="Number" 
                        showValueHelp="true" valueHelpIconSrc="sap-icon://add" valueHelpRequest=".searchOrder" 
                        liveChange=".hasOnlyNumbers" width="auto" />
                    <ToolbarSpacer/>
                    <Input id="searchPosition" width="auto" textAlign="Initial" placeholder="{appTxts>/quotes.search}" 
                        type="Number" showValueHelp="true" valueHelpIconSrc="sap-icon://search" 
                        valueHelpRequest=".filterOrderPositions" change=".filterOrderPositions" />
                </OverflowToolbar>
            </headerToolbar>
            <columns>
                <Column width="auto" hAlign="Left">
                    <Text text="{appTxts>/quotes.order}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column width="auto" hAlign="Left">
                    <Text text="{appTxts>/quotes.startShipment}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                    <Text text="{appTxts>/quotes.endShipment}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>
                <Column width="auto" hAlign="Left" vAlign="Top" minScreenWidth="Tablet" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                    <Text text="{appTxts>/quotes.prevQuotes}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                </Column>                
            </columns>
            <items>
                <ColumnListItem press="onListItemPress" selected="{tableWizardPo_validas>Selected}" visible="{= ${tableWizardPo_validas>Cresta} > 0}">
                    <cells>
                        <ObjectIdentifier text="{tableWizardPo_validas>Ebelp}" />
                        <ObjectIdentifier text="{tableWizardPo_validas>Ean11}" />
                        <ObjectIdentifier text="{tableWizardPo_validas>Txz01}" />                                                       
                        <ObjectIdentifier text="{tableWizardPo_validas>Cresta}"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </Panel>
</VBox>
</core:FragmentDefinition>