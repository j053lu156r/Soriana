<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc">
	<Dialog id="dinamicTableNE" title="{appTxts>/visor.msgPopOption} " draggable="true">
		<l:VerticalLayout>
		
			<CheckBox text="{appTxts>/visor.msgSelectAll}"
				selected="{= ${/idMblnr} || ${/idEbeln} || ${/idBudatMkpf}|| ${/idWerks}|| ${/idXblnr}|| ${/idXblnr2}}"
				partiallySelected="{= !(${/idMblnr} &amp;&amp; ${/idEbeln} &amp;&amp; ${/idBudatMkpf}&amp;&amp; ${/idWerks}&amp;&amp; ${/idXblnr}&amp;&amp; ${/idXblnr2})}"
				select="onParentClicked"/>
			<c:HTML content="&lt;hr&gt;"/>
			<CheckBox text="{appTxts>/visor.folio}" selected="{/idMblnr}"/>
			<CheckBox text="{appTxts>/visor.order}" selected="{/idEbeln}"/>
			<CheckBox text="{appTxts>/visor.receivedDate}" selected="{/idBudatMkpf}"/>
			<CheckBox text="{appTxts>/visor.branchOffice}" selected="{/idWerks}"/>
			<CheckBox text="{appTxts>/visor.Frecibo}" selected="{/idXblnr}"/>
            <CheckBox text="{appTxts>/visor.invoice}" selected="{/idXblnr2}"/>
			
		
		
			
		</l:VerticalLayout>
		<buttons>
			<Button text="{appTxts>/visor.msgCloseOption}" press="ClosepopUp"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>