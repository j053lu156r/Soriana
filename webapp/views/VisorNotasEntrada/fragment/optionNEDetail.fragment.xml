<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:c="sap.ui.core" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc">
	<Dialog id="dinamicTableNED" title="{appTxts>/visor.msgPopOption} " draggable="true">
		<l:VerticalLayout>
		
		<CheckBox text="{appTxts>/visor.msgSelectAll}"
				selected="{= ${/idEbelp} || ${/idEan11} || ${/idMaktx}|| ${/idErfmg}|| ${/idFconver}|| ${/idMenge}}"
				partiallySelected="{= !(${/idEbelp} &amp;&amp; ${/idEan11} &amp;&amp; ${/idMaktx}&amp;&amp; ${/idErfmg}&amp;&amp; ${/idFconver}&amp;&amp; ${/idMenge})}"
				select="onParentClicked"/>
			<c:HTML content="&lt;hr&gt;"/>
			<CheckBox text="{appTxts>/visor.position}" selected="{/idEbelp}"/>
			<CheckBox text="{appTxts>/visor.codigo}" selected="{/idEan11}"/>
			<CheckBox text="{appTxts>/visor.descripcion}" selected="{/idMaktx}"/>
			<CheckBox text="{appTxts>/visor.quantity}" selected="{/idErfmg}"/>
			<CheckBox text="{appTxts>/visor.Fconver}" selected="{/idFconver}"/>
            <CheckBox text="{appTxts>/visor.capacidad}" selected="{/idMenge}"/>




			
		</l:VerticalLayout>
		<buttons>
			<Button text="{appTxts>/visor.msgCloseOption}" press="ClosepopUp"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>