<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		contentWidth= "450px"
		contentHeight= "300px"
		noDataText="{appTxts>/supplier.noData}"
		title="{appTxts>/supplier.title}"
		search="handleSearch"
		confirm="handleClose"
		cancel="handleClose"
		multiSelect="true" 
		items="{
			path: '/name'
		}" >
		<StandardListItem
			title="{C}"
			description="{D}"
			iconDensityAware="false"
			iconInset="false"
			type="Active"
			/>
	</SelectDialog>
</core:FragmentDefinition>