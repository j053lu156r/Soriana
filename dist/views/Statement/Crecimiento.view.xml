<mvc:View displayBlock="true" 
    xmlns:f="sap.f" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar" 
    xmlns:vm="sap.ui.comp.variants" 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
     xmlns:t ="sap.ui.table"
	 controllerName="demo.controllers.Statement.Crecimiento" height="100%">
	<f:DynamicPage  
		class="sapUiContentPadding">
        <!-- DynamicPage Title -->
      	<f:title>
			<f:DynamicPageTitle>
				<f:heading>
              
					<Title text="{detailAcuerdosAS>/document}"/>
				</f:heading>
				<!--
					<f:breadcrumbs>
						<Breadcrumbs>
							<Link text="Home" />
							<Link text="Page 1" />
							<Link text="Page 2" />
							<Link text="Page 3" />
							<Link text="Page 4" />
							<Link text="Page 5" />
						</Breadcrumbs>
					</f:breadcrumbs>
				-->
				<f:expandedContent>
					<Label text="{detailAcuerdosAS>/document}"/>
				</f:expandedContent>
				<f:snappedContent>
					<Label text="{detailAcuerdosAS>/document}"/>
				</f:snappedContent>
				<f:snappedTitleOnMobile>
					<Title text="{detailAcuerdosAS>/document}"/>
				</f:snappedTitleOnMobile>


				<f:navigationActions>
					<Button icon="sap-icon://full-screen" type="Transparent" press="handleFullScreen" id="enterFullScreenBtn" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }" />
					<Button icon="sap-icon://exit-full-screen" type="Transparent" press="handleExitFullScreen" id="exitFullScreenBtn" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }" />
					<Button icon="sap-icon://decline" type="Transparent" press="handleClose" tooltip="Close column" />
				</f:navigationActions>
			</f:DynamicPageTitle>
		</f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
               

                </f:content>
            </f:DynamicPageHeader>
        </f:header>
		<f:content>
            <VBox >
                
                <Table 
                    growing="true" 
                    growingScrollToLoad="true"
                    growingThreshold="10"
                    noDataText="{appTxts>/polizas.noData}" 
                    id="tableEscalas2"
                    items="{EscalasDta>/}">
                    <headerToolbar>
                        <OverflowToolbar >
                            <Title  text="{appTxts>/polizas.Position} ({EscalasDta>/length})" />
                            <ToolbarSpacer />
    
                          
                            <Button  icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" 
                                press=".buildExportTable"
                                visible="{= ${EscalasDta>/length} > 0}"
                            />
                            <Select 
                                visible="{= ${EscalasDta>/length} > 0 }"
                                change=".paginateValue(${$parameters>/selectedItem}, 'EscalasDta', '/')">
                                <items>
                                    <core:Item  key="10" text="10"/>
                                    <core:Item  key="20" text="20"/>
                                    <core:Item  key="50" text="50"/>
                                    <core:Item  key="100" text="100"/>
                                </items>
                            </Select>
                        </OverflowToolbar>
                    </headerToolbar>

                    <columns>
                        <Column  width="auto" hAlign="Center" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.arrangement}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <Text  text="Total" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </footer>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.matdoc}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.matdocYear}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.movClass}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.reference}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.plant}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.amount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <ObjectNumber
                                    number="{
                                        parts:[{path:'escalaTotModel>/totImporte'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{escalaTotModel>/currCode}" />
                            </footer>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.perDescount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.Descount}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'escalaTotModel>/TotDescto'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{escalaTotModel>/currCode}" />
                            </footer>
                        </Column>

                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.IVA}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'escalaTotModel>/totIVA'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{escalaTotModel>/currCode}" />
                            </footer>
                        </Column>
                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text  text="{appTxts>/ACEscalas.IEPS}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'escalaTotModel>/totIEPS'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{escalaTotModel>/currCode}" />
                            </footer>
                        </Column>

                        <Column  width="auto" hAlign="Right" vAlign="Top" minScreenWidth="Phone" demandPopin="true" popinDisplay="Inline" mergeDuplicates="false">
                            <header>
                                <Text text="{appTxts>/ACEscalas.subtotal}" width="auto" maxLines="1" wrapping="false" textAlign="Begin" textDirection="Inherit"/>
                            </header>
                            <footer>
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'escalaTotModel>/TotStot'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{escalaTotModel>/currCode}" />
                            </footer>
                        </Column>
                    </columns>

                    <items>
                        <ColumnListItem >                    
                            <cells>
                                <ObjectIdentifier 
                                    title="{EscalasDta>knuma}" 
                                    titleActive="false"/>
                                <ObjectIdentifier
                                    text="{EscalasDta>mblnr}" />
                                <ObjectIdentifier
                                    text="{EscalasDta>mjahr}" />
                                <ObjectIdentifier
                                    text="{EscalasDta>bwart}" />
                                <ObjectIdentifier
                                    text="{EscalasDta>xblnr}" />
                                <ObjectIdentifier
                                    text="{EscalasDta>werks}" />
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'EscalasDta>dmbtr'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{EscalasDta>waers}" />
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'EscalasDta>zboni'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{EscalasDta>konwa}" />
                                <ObjectIdentifier
                                    text="{EscalasDta>zmgn3}" />

                                <ObjectNumber
                                    number="{
                                        parts:[{path:'EscalasDta>ziva'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{EscalasDta>waers}" />
                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'EscalasDta>zieps'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{EscalasDta>waers}" />

                                <ObjectNumber 
                                    number="{
                                        parts:[{path:'EscalasDta>stotal'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                            }"
                                    unit="{EscalasDta>waers}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
                
                <OverflowToolbar id="OFT" visible="{= ${EscalasDta>/results/length} > 0 }">
                    <Text id="txtIR" text="{EscalasDta>/initRow} - {EscalasDta>/endRow} {appTxts>/pagination.of} {EscalasDta>/results/length}" />
                    <ToolbarSpacer id="TBS"/>
                    <Button id="BTNL" type="Default" icon="sap-icon://slim-arrow-left" 
                            press=".prevPagination(${EscalasDta>/iterator}, ${EscalasDta>/initRow}, 'EscalasDta', '/PolizasDet')" 
                            enabled="{= ${EscalasDta>/iterator} !== 1}" />
                    <Button id="BTNR" type="Default" icon="sap-icon://slim-arrow-right" 
                            press=".nextPagination(${EscalasDta>/PolizasDet/Paginated/iterator}, ${EscalasDta>/PolizasDet/Paginated/endRow}, 'EscalasDta', '/PolizasDet')" 
                            enabled="{= ${EscalasDta>/PolizasDet/Paginated/endRow} !== ${EscalasDta>Det/results/length}}" />
                </OverflowToolbar>

            </VBox>
		</f:content>
	</f:DynamicPage>
</mvc:View>