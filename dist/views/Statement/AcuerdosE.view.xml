<mvc:View displayBlock="true" 
    xmlns:f="sap.f" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar" 
    xmlns:vm="sap.ui.comp.variants" 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core"
     xmlns:t ="sap.ui.table"
	 controllerName="demo.controllers.Statement.AcuerdosE" height="100%">
	<f:DynamicPage  
		class="sapUiContentPadding">
        <!-- DynamicPage Title -->
      	<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="{detailAcuerdosAS>/document}"/>
				</f:heading>
				<!--
					<f:breadcrumbs>
						<Breadcrumbs>
							<Link text="Home" />
							<Link text="Page 1" />
							<Link text="Page 2" />
							<Link text="Page 3" />
							<Link text="Page 4" />
							<Link text="Page 5" />
						</Breadcrumbs>
					</f:breadcrumbs>
				-->
				<f:expandedContent>
					<Label text="{detailAcuerdosAS>/document}"/>
				</f:expandedContent>
				<f:snappedContent>
					<Label text="{detailAcuerdosAS>/document}"/>
				</f:snappedContent>
				<f:snappedTitleOnMobile>
					<Title text="{detailAcuerdosAS>/document}"/>
				</f:snappedTitleOnMobile>


				<f:navigationActions>
					<Button icon="sap-icon://full-screen" type="Transparent" press="handleFullScreen" id="enterFullScreenBtn" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }" />
					<Button icon="sap-icon://exit-full-screen" type="Transparent" press="handleExitFullScreen" id="exitFullScreenBtn" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }" />
					<Button icon="sap-icon://decline" type="Transparent" press="handleClose" tooltip="Close column" />
				</f:navigationActions>
			</f:DynamicPageTitle>
		</f:title>
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
               

                </f:content>
            </f:DynamicPageHeader>
        </f:header>
		<f:content>
            <VBox>
			   <t:Table 
                    noData                  ="{appTxts>/pay.merchandise}" 
                    id                      ="aclaracionesList"
                    alternateRowColors      ="true" 
                    rows                    ="{ECuentaAcl>/}"
                    enableColumnReordering  = "false"
                    enableSelectAll         ="false"
                    visibleRowCount="10"
                    selectionMode           ="None">  
                    <t:extension>
                        <OverflowToolbar style="Clear">
                            <Title id="title" text="{appTxts>/stateA.title} ({ECuentaAcl>/length})"/>
                            <ToolbarSpacer/>
                            <Button icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" press=".buildExportTable" visible="{= ${ECuentaAcl>/length} > 0}" />
                      
                         
                        </OverflowToolbar>
                    </t:extension>
                    <t:columns>
                        <t:Column width="auto" id="idLifnr" visible="true" sortProperty="Lifnr" filterProperty="Lifnr">
                            <Label text="{appTxts>/stateA.proveedor}" />
                            <t:template>
                                <Text text="{ECuentaAcl>Lifnr}" wrapping="false"/>
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idBelnr" visible="true" sortProperty="Belnr" filterProperty="Belnr" >
                            <Label text="{appTxts>/stateA.documento}" />
                            <t:template>
                                <Text text="{ECuentaAcl>Belnr}" wrapping="false"/>
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idConve" visible="true" sortProperty="Conve" filterProperty="Conve">
                            <Label text="{appTxts>/stateA.convenio}" />
                            <t:template>
                                <Text text="{ECuentaAcl>Conve}" wrapping="false"/>
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idWerks" visible="true" sortProperty="Werks" filterProperty="Werks">
                            <Label text="{appTxts>/stateA.centro}" />
                            <t:template>
                                <Text text="{ECuentaAcl>Werks}" wrapping="false"/>
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idFolio" visible="true" sortProperty="Folio" filterProperty="Folio">
                            <Label text="{appTxts>/stateA.folio}" />
                            <t:template>
                                <Text text="{ECuentaAcl>Folio}" wrapping="false"/>
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idEbeln" visible="true" sortProperty="Ebeln" filterProperty="Ebeln">
                            <Label text="{appTxts>/stateA.pedido}" />
                            <t:template>
                              <Text text="{ECuentaAcl>Ebeln }" wrapping="false"/>
                              </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idBase" visible="true" sortProperty="Base" filterProperty="Base">
                            <Label text="{appTxts>/stateA.base}" />
                            <t:template>
                            <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Base'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            <!--<Text text="{ECuentaAcl>Base }" wrapping="false"/>--> 
                           </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idMatnr" visible="true" sortProperty="Matnr" filterProperty="Matnr">
                            <Label text="{appTxts>/stateA.material}" />
                            <t:template>
                          <ObjectIdentifier title="{ECuentaAcl>Matnr}" text="{ECuentaAcl>Maktx}" />
                            </t:template>
                        </t:Column>
                          <t:Column width="auto" id="idCanti" visible="false" sortProperty="Canti" filterProperty="Canti">
                            <Label text="{appTxts>/stateA.cantidad}" />
                            <t:template>
                                <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Canti'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit=""
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idPvmat" visible="true" sortProperty="Pvmat" filterProperty="Pvmat">
                            <Label text="{appTxts>/stateA.pvMat}" />
                            <t:template>
                               <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Pvmat'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="Cumat" visible="true" sortProperty="Cumat" filterProperty="Cumat">
                            <Label text="{appTxts>/stateA.cuMat}" />
                            <t:template>
                                <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Cumat'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idDesct" visible="true" sortProperty="Desct" filterProperty="Desct">
                            <Label text="{appTxts>/stateA.descto}" />
                            <t:template>
                             <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Desct'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                                <!-- <Text text="{ECuentaAcl> }" wrapping="false"/>-->
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idCargo" visible="true" sortProperty="Cargo" filterProperty="Cargo">
                            <Label text="{appTxts>/stateA.cargo}" />
                            <t:template>
                              <Text text="{ECuentaAcl>Cargo }" wrapping="false"/>
                             </t:template>
                        </t:Column>
                          <t:Column width="auto" id="idIeps" visible="true" sortProperty="Ieps" filterProperty="Ieps">
                            <Label text="{appTxts>/stateA.ieps}" />
                            <t:template>
                               <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Ieps'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                        <t:Column width="auto" id="idIva" visible="true" sortProperty="Iva" >
                            <Label text="{appTxts>/stateA.iva}" />
                            <t:template>
                             <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Iva'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            
                               </t:template>
                        </t:Column>
                      
                         <t:Column width="auto" id="idPrdes" visible="true" sortProperty="Prdes" filterProperty="Prdes">
                            <Label text="{appTxts>/stateA.pDescto}" />
                            <t:template>
                               <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Prdes'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="%"
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                         <t:Column width="auto" id="idFecca"  visible="true" sortProperty="Fecca" filterProperty="Fecca">
                            <Label text="{appTxts>/stateA.fecCargo}" />
                            <t:template>
                                <Text
                                    text="{ 
                                            path: 'ECuentaAcl>Fecca'
                                            }"
                                />
                            </t:template>
                        </t:Column>
                         <t:Column width="auto" id="idTotca" visible="true" sortProperty="Totca" filterProperty="Totca">
                            <Label text="{appTxts>/stateA.totCargo}" />
                            <t:template>
                               <ObjectNumber
                                    emphasized="false"
                                    number="{
                                        parts:[{path:'ECuentaAcl>Totca'},{path:'CurrencyCode'}],
                                        type: 'sap.ui.model.type.Currency',
                                        formatOptions: {showMeasure: false}
                                        }"
                                    unit="MXN"
                                    state="None"
                                />
                            </t:template>
                        </t:Column>
                    </t:columns>
                  
                </t:Table>
            </VBox>
		</f:content>
	</f:DynamicPage>
</mvc:View>