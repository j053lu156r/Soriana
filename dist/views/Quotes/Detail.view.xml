<mvc:View displayBlock="true" 
        controllerName="demo.controllers.Quotes.Detail" 
        height="100%"
		xmlns="sap.uxap"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:m="sap.m"
		xmlns:f="sap.f"
        xmlns:core="sap.ui.core" 
		xmlns:form="sap.ui.layout.form">

	<ObjectPageLayout id="ObjectPageLayout"
					 showTitleInHeaderContent="true"
					alwaysShowContentHeader="false"
					preserveHeaderStateOnScroll="false"
					headerContentPinnable="true"
					isChildPage="true"
					upperCaseAnchorBar="false">
				<headerTitle>
					<ObjectPageDynamicHeaderTitle>
						<expandedHeading>
                            <m:Title text="{appTxts>/quotes.quoteFolio}: {tableQuotesDetail>/ECITASCONSNAV/results/0/ZfolioCita} " wrapping="true" class="sapUiSmallMarginEnd"/>
						</expandedHeading>

						<snappedHeading>
							<m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Center">
								<m:FlexBox wrap="NoWrap" fitContainer="true" alignItems="Center" class="sapUiTinyMarginEnd">
									<f:Avatar src="../../../../../../../{products>ProductPicUrl}" displaySize="S" displayShape="Square"/>
									<m:Title text="{products>Name}" wrapping="true" class="sapUiTinyMarginEnd"/>
								</m:FlexBox>
							</m:FlexBox>
						</snappedHeading>

						<navigationActions>
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://full-screen" press="handleFullScreen" id="enterFullScreenBtn" tooltip="Enter Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/fullScreen} !== null }"/>
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://exit-full-screen" press="handleExitFullScreen" id="exitFullScreenBtn" tooltip="Exit Full Screen Mode" visible="{= ${/actionButtonsInfo/midColumn/exitFullScreen} !== null }"/>
							<m:OverflowToolbarButton type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close column" visible="{= ${/actionButtonsInfo/midColumn/closeColumn} !== null }" />
						</navigationActions>

                        <actions>
                            <m:Button 
                                    text="{appTxts>/quotes.acceptAppointment}"
                                    type="Accept"
                                    visible="{= ${tableQuotesDetail>/ECITASCONSNAV/results/0/Znumstatus} === '02'}"
                                    press=".printLabels"/>
                            <m:Button
                                    text="{appTxts>/quotes.cancelAppointment}"
                                    type="Reject"
                                    visible="{= ${tableQuotesDetail>/ECITASCONSNAV/results/0/Znumstatus} === '03'}"
                                    press=".hancleCandel(${tableQuotesDetail>/ECITASCONSNAV/results/0/ZfolioCita}, 2)"
                                     />
                            <m:Button 
                                    text="{appTxts>/quotes.rejectAppointment}"
                                    type="Transparent"
                                    visible="{= ${tableQuotesDetail>/ECITASCONSNAV/results/0/Znumstatus} === '02'}"
                                    press=".hancleCandel(${tableQuotesDetail>/ECITASCONSNAV/results/0/ZfolioCita}, 3)"
                                    />
                            <m:Button 
                                    text="{appTxts>/quotes.editAppointment}"
                                    type="Transparent"
                                    visible="{= ${tableQuotesDetail>/ECITASCONSNAV/results/0/Znumstatus} !== '06' &amp;&amp; ${tableQuotesDetail>/ECITASCONSNAV/results/0/Znumstatus} !== '04'}"
                                    press=".handleEditBtn(${tableQuotesDetail>/ECITASCONSNAV/results/0/ZfolioCita})"/>
                            <m:Button 
                                    text="{appTxts>/quotes.quoteCard}"
                                    icon="sap-icon://qr-code"
                                    type="Transparent"
                                    press=".printQuoteCard"/>
                        </actions>

					</ObjectPageDynamicHeaderTitle>
				</headerTitle>

				<headerContent>
					<m:FlexBox wrap="Wrap" fitContainer="true" alignItems="Stretch" class="sapUiSmallMarginBottom">
						<f:Avatar src="sap-icon://monitor-payments"
								  displaySize="L"
								  displayShape="Square"
								  class="sapUiTinyMarginEnd">
						</f:Avatar>
                        <m:VBox justifyContent="Center" class="sapUiSmallMarginEnd">
                            <m:Label text="{appTxts>/quotes.branch}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zsucursal} - {tableQuotesDetail>/ECITASCONSNAV/results/0/Zdescrsucursal}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.platform}" />
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zanden}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.turn}" />
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zturno} - {tableQuotesDetail>/ECITASCONSNAV/results/0/ZTipoturno}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.status}" />
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zstatus}" class="sapUiTinyMarginBegin"/>
                        <m:layoutData>
						    <m:FlexItemData growFactor="1" />
					    </m:layoutData>
                        </m:VBox>
						<m:VBox justifyContent="Center" class="sapUiSmallMarginEnd">
                            <m:Label text="{appTxts>/quotes.quoteType}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtipoCita}" class="sapUiTinyMarginBegin"/>
							<m:Label text="{appTxts>/quotes.productType}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtipoProd}" class="sapUiTinyMarginBegin"/>
							<m:Label text="{appTxts>/quotes.unitType}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtipoUnidad}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.assortmentType}" />
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtipoSurtido}" class="sapUiTinyMarginBegin"/>
                        <m:layoutData>
						    <m:FlexItemData growFactor="1" />
					    </m:layoutData>
						</m:VBox>
                        <m:VBox justifyContent="Center" class="sapUiSmallMarginEnd">
                            <m:Label text="{appTxts>/quotes.codesNum}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZnumCodigos}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.totalpackages}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtotalBultoped}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.packagesReceived}"/>
							<m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZtotalBultorec}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.weight}" />
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zpeso}" class="sapUiTinyMarginBegin"/>
                        <m:layoutData>
						    <m:FlexItemData growFactor="1" />
					    </m:layoutData>
                        </m:VBox>
                        <m:VBox justifyContent="Center" class="sapUiSmallMarginEnd">
                            <m:Label text="{appTxts>/quotes.quoteDate}"/>
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zfecharegcita}" class="sapUiTinyMarginBegin"/>
							<m:Label text="{appTxts>/quotes.dateReceipt}"/>
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/ZfechaRecibo}" class="sapUiTinyMarginBegin sapUiTinyMarginEnd"/>
                            <m:Label text="{appTxts>/quotes.boardingStartDate}"/>
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zfechainiemb}" class="sapUiTinyMarginBegin"/>
                            <m:Label text="{appTxts>/quotes.shipmentEndDate}"/>
                            <m:Text text="{tableQuotesDetail>/ECITASCONSNAV/results/0/Zfechafinemb}" class="sapUiTinyMarginBegin"/>
                        <m:layoutData>
						    <m:FlexItemData growFactor="1" />
					    </m:layoutData>
                        </m:VBox>
					</m:FlexBox>
				</headerContent>

		<sections>
			<ObjectPageSection title="{appTxts>/quotes.orders}">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
                            <m:VBox>
							<m:Table id="suppliersTable"
									 items="{path : 'tableQuotesDetail>/ECITASDETNAV/Paginated/results'}">
                                     <m:headerToolbar>
                                        <m:OverflowToolbar>
                                             <m:ToolbarSpacer/>
                                            <m:Button icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" press=".buildExcelOrders" visible="{= ${tableDetailMoves>/EDOCDTLNAV/results/length} > 0}" />
                                            <m:Select 
                                                visible="{= ${tableQuotesDetail>/ECITASDETNAV/Paginated/results/length} > 0 }"
                                                change=".paginateValue(${$parameters>/selectedItem}, 'tableQuotesDetail', '/ECITASDETNAV')">
                                                <m:items>
                                                    <core:Item key="10" text="10"/>
                                                    <core:Item key="20" text="20"/>
                                                    <core:Item key="50" text="50"/>
                                                    <core:Item key="100" text="100"/>
                                                </m:items>
                                            </m:Select>
                                        </m:OverflowToolbar>
                                    </m:headerToolbar>
								<m:columns>
									<m:Column><m:Text text="{appTxts>/quotes.order}" /></m:Column>
                                    <m:Column><m:Text text="{appTxts>/quotes.branch}" /></m:Column>
                                    <m:Column><m:Text text="{appTxts>/quotes.totalPackageOrdered}" /></m:Column>
                                    <m:Column><m:Text text="{appTxts>/quotes.totalPackageReceive}" /></m:Column>
								</m:columns>
								<m:items>
									<m:ColumnListItem type="Navigation" press="handleItemPress">
										<m:cells>
                                            <m:ObjectIdentifier text="{tableQuotesDetail>ZPedido}" />
                                            <m:ObjectIdentifier text="{tableQuotesDetail>ZSucursal} - {tableQuotesDetail>Zdescrsucursal}" />
                                            <m:ObjectIdentifier text="{tableQuotesDetail>ZTotalbultoped}" />
                                            <m:ObjectIdentifier text="{tableQuotesDetail>ZTotalbultorec}" />
										</m:cells>
									</m:ColumnListItem>
								</m:items>
							</m:Table>

                            <m:OverflowToolbar visible="{= ${tableQuotesDetail>/ECITASDETNAV/Paginated/results/length} > 0 }">
                                <m:Text text="{tableQuotesDetail>/ECITASDETNAV/Paginated/initRow} - {tableQuotesDetail>/ECITASDETNAV/Paginated/endRow} {appTxts>/pagination.of} {tableQuotesDetail>/ECITASDETNAV/results/length}" />
                                <m:ToolbarSpacer/>
                                <m:Button type="Default" icon="sap-icon://slim-arrow-left" press=".prevPagination(${tableQuotesDetail>/ECITASDETNAV/Paginated/iterator}, ${tableQuotesDetail>/ECITASDETNAV/Paginated/initRow}, 'tableQuotesDetail', '/ECITASDETNAV')" enabled="{= ${tableQuotesDetail>/ECITASDETNAV/Paginated/iterator} !== 1}" />
                                <m:Button type="Default" icon="sap-icon://slim-arrow-right" press=".nextPagination(${tableQuotesDetail>/ECITASDETNAV/Paginated/iterator}, ${tableQuotesDetail>/ECITASDETNAV/Paginated/endRow}, 'tableQuotesDetail', '/ECITASDETNAV')" enabled="{= ${tableQuotesDetail>/ECITASDETNAV/Paginated/endRow} !== ${tableQuotesDetail>/ECITASDETNAV/results/length}}" />
                            </m:OverflowToolbar>
                            </m:VBox>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>