<mvc:View displayBlock="true" 
    xmlns:f="sap.f" 
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar" 
    xmlns:vm="sap.ui.comp.variants" 
    xmlns:unified="sap.ui.unified"
    xmlns="sap.m" 
      xmlns:t="sap.ui.table"
    xmlns:core="sap.ui.core"
    controllerName="demo.controllers.ValidacionCondiciones.MasterValidaciones"
    height="100%">
    <f:DynamicPage class="sapUiContentPadding">
        <f:title>
            <f:DynamicPageTitle backgroundDesign="Transparent">
                <f:heading>
                    <vm:VariantManagement standardItemText="{appTxts>/ValCondi.title}"/>
                </f:heading>
            </f:DynamicPageTitle>
        </f:title>
        
        <f:header>
            <f:DynamicPageHeader pinnable="true">
                <f:content>
                    <fb:FilterBar id="filterBar"  header="Header" reset="onReset" search="searchData2" 
                        clear="clearFilters" cancel="onCancel" filtersDialogClosed = "onFiltersDialogClosed" 
                        showRestoreButton="false" showClearButton="false" useToolbar="false">

                            <fb:filterGroupItems>
                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="Ab" 
                                    label="{appTxts>/ValCondi.Acreedor}" mandatory="true" 
                                    partOfCurrentVariant="true" visibleInFilterBar="true">
                                    <fb:control>
                                        <Input
                                            id="supplierInput" placeholder="{appTxts>/ValCondi.Acreedor}" 
                                            showSuggestion="true" valueHelpOnly="true"
                                            showValueHelp="true" valueHelpRequest=".onValueHelpRequest" 
                                            value="{configSite>/supplierInput}" >
                                        </Input>
                                    </fb:control>
                                </fb:FilterGroupItem>

                                <fb:FilterGroupItem groupName="F1" label="{appTxts>/ValCondi.Ifecha}" 
                                    groupTitle="Fechas" name="Fecha de entrega" mandatory="false" 
                                    partOfCurrentVariant="true" visibleInFilterBar="true">
                                    <fb:control>
                                        <DateRangeSelection id="dateRange" required="false"/>
                                    </fb:control>
                                </fb:FilterGroupItem>

                                <fb:FilterGroupItem groupName="__$INTERNAL$" name="Invoice2" 
                                    label="{appTxts>/ValCondi.Folio}" mandatory="false" 
                                    partOfCurrentVariant="true" visibleInFilterBar="true">
                                    <fb:control>
                                     
                                        <Input
                                            id="IDFolio" placeholder="" liveChange=""/>
                                       
                                    </fb:control>
                                    
                                </fb:FilterGroupItem>
                                 <fb:FilterGroupItem groupName="__$INTERNAL$" name="Invoice" 
                                    label="{appTxts>/ValCondi.Iserie}" mandatory="false" 
                                    partOfCurrentVariant="true" visibleInFilterBar="true">
                                    <fb:control>
                                     
                                        <Input
                                            id="IDSerie" placeholder="" liveChange=""/>
                                    </fb:control>
                                    
                                </fb:FilterGroupItem>
                            </fb:filterGroupItems>
                    </fb:FilterBar>
                </f:content>
            </f:DynamicPageHeader>

        </f:header>
        <f:content>
           <VBox>
            <t:Table selectionMode="None" noData="{appTxts>/acuerdos.noData}" id="tableValidacion" rows="{ModelValidacion>/}">
                    <t:extension>
                        <OverflowToolbar> 
                            <Title text="Validación de condiciones ({ModelValidacion>/length})" />
                            <ToolbarSpacer/>

                            <Button icon="sap-icon://excel-attachment" text="{appTxts>/global.export}" press=".buildExportTable" visible="{= ${ModelValidacion>/length} > 0}" />
      <Button icon="sap-icon://action-settings" press="ConfigTable" />
                  
                        </OverflowToolbar>
                    </t:extension>
                    <t:columns>
